{
	"name": "eslint-config-wikimedia",
	"version": "0.21.0",
	"description": "ESLint config following Wikimedia code conventions.",
	"main": "common.json",
	"scripts": {
		"test": "npm run lint && qunit test/test.js",
		"lint": "eslint . --max-warnings 0",
		"build-invalid": "node test/test.js dump"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wikimedia/eslint-config-wikimedia.git"
	},
	"homepage": "https://www.mediawiki.org/wiki/Manual:Coding_conventions/JavaScript",
	"bugs": "https://github.com/wikimedia/eslint-config-wikimedia/issues",
	"keywords": [
		"style guide",
		"eslint",
		"eslintconfig",
		"wikimedia"
	],
	"engine": {
		"node": ">=12"
	},
	"files": [
		"common.json",
		"client.json",
		"client-common.json",
		"client-es5.js",
		"client-es6.json",
		"node.json",
		"jquery.json",
		"jsdoc.json",
		"jsduck.json",
		"json.json",
		"mediawiki.js",
		"mocha.json",
		"qunit.json",
		"selenium.json",
		"server.json",
		"vue-common.json",
		"vue2-common.json",
		"vue3-common.json",
		"vue-es5.json",
		"vue-es6.json",
		"vue2-es5.js",
		"vue2-es6.js",
		"vue3-es6.js",
		"vue-wrappers.js",
		"language"
	],
	"contributors": [
		"James D. Forrester <jforrester@wikimedia.org>",
		"Ed Sanders <ejsanders@gmail.com>",
